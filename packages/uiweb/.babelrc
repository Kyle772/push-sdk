{
  "presets": [
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic",
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    ["styled-components", { "pure": true, "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/lib/components"
          // Add more aliases as needed
        }
      }
    ]
  ]
}
